{
  "name": "PROJECT_NAME",
  "version": "1.0.0",
  "description": "PROJECT_DESCRIPTION",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src config",
    "type-check": "yarn run flow",
    "prettify": "yarn run prettier --write \"{src,config}/**/*.js\"",
    "precommit": "lint-staged && yarn lint && yarn type-check && yarn test",
    "build:client": "webpack --config config/webpack/client.js",
    "build:production": "cross-env-shell NODE_ENV=production \"yarn build:client\"",
    "dev": "cosmos --config config/cosmos.config.js",
    "cosmos:export": "cosmos-export"
  },
  "jest": {
    "rootDir": "",
    "modulePathIgnorePatterns": [
      "<rootDir>/packages/",
      "<rootDir>/server/",
      "<rootDir/node_modules/"
    ],
    "testRegex": "(src|config).*(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "commander": "^2.11.0",
    "cross-env": "^5.1.1",
    "emotion": "^8.0.9",
    "express": "^4.16.2",
    "inquirer": "^3.3.0",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-emotion": "^8.0.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "eslint": "^4.10.0",
    "eslint-config-dqnt": "^2.1.0",
    "flow-bin": "^0.57.3",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "preload-webpack-plugin": "^2.0.0",
    "prettier": "1.7.4",
    "react-cosmos": "^3.0.0",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-features": "^3.1.4",
    "webpack-hot-middleware": "^2.20.0",
    "why-did-you-update": "^0.1.0"
  }
}
